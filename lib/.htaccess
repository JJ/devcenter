ErrorDocument 404 /404.html

RewriteEngine on


# Rewrite help.openshift.com to developers.openshift.com
RewriteCond %{HTTP_HOST} ^help.openshift.com$
RewriteRule (.*)$ https://developers.openshift.com/$1 [R,L]

# Rewrite http to https
RewriteCond %{HTTP:X-Forwarded-Proto} !https
RewriteRule .* https://%{HTTP_HOST}%{REQUEST_URI} [R,L]

# Search Rewrite
# https://help.openshift.com/hc/en-us/search?refinement=documentation&query=searched
RewriteRule ^hc/en-us/search(.*) /en/search.html$1 [L,R=301,NC]

